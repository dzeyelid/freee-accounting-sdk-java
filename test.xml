<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="target">
      <target name="body">Hello world</target>
      <target key="nested.body">How are you?</target>
    </section>
  </configSections>
  <connectionStrings>
    <add name="body" connectionString="Hello world" />
    <add name="nested.body" connectionString="How are you?" />
  </connectionStrings>
  <appSettings>
    <add key="body" value="Hollo world" />
    <add key="nested.body" value="How are you?" />
  </appSettings>
  <applicationSettings>
    <myCustomSection>
      <setting name="body" serializeAs="String">
          <value>Hello world</value>
      </setting>
      <setting name="nested.body" serializeAs="String">
          <value>How are you?</value>
      </setting>
    </myCustomSection>
  </applicationSettings>
  <contents>
  </contents>
</configuration>
