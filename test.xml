<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="targetElements">
  </configSections>
  <connectionStrings>
    <add name="body" connectionString="Hello world" />
    <add name="nested.body" connectionString="How are you?" />
  </connectionStrings>
  <appSettings>
    <add key="body" value="Hollo world" />
    <add key="nested.body" value="How are you?" />
  </appSettings>
  <applicationSettings>
    <myCustomSection>
      <setting name="body" serializeAs="String">
          <value>Hello world</value>
      </setting>
      <setting name="nested.body" serializeAs="String">
          <value>How are you?</value>
      </setting>
    </myCustomSection>
  </applicationSettings>
  <targetElements>
    <property body="Hollo world" />
  </targetElements>
</configuration>
